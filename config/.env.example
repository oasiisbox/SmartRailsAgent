# SmartRailsAgent Environment Variables
# Part of the OASIISBOX SmartRails Suite

# API Keys - Set these in your environment
OPENAI_API_KEY=your-openai-api-key-here
MISTRAL_API_KEY=your-mistral-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Ollama Configuration
OLLAMA_HOST=localhost
OLLAMA_PORT=11434

# SmartRailsAgent Settings
SMARTRAILS_AGENT_PROVIDER=openai
SMARTRAILS_AGENT_TIMEOUT=30
SMARTRAILS_AGENT_DEBUG=false

# SmartRails Integration
SMARTRAILS_AUDIT_AI=false
SMARTRAILS_CACHE_RESPONSES=false
SMARTRAILS_LOG_REQUESTS=false